<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/border">


    <TextView
        android:id="@+id/tvtitle"
        android:layout_width="283dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="21dp"
        android:layout_toStartOf="@+id/prof"
        android:fontFamily="@font/abeezee"
        android:paddingLeft="15dp"
        android:paddingTop="6dp"
        android:paddingBottom="5dp"
        android:text="hello"
        android:textColor="#120F0F"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvtitle2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvtitle"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="0dp"
        android:fontFamily="sans-serif-condensed"
        android:paddingLeft="15dp"
        android:paddingBottom="5dp"
        android:text="address"
        android:textColor="#0C0A0A"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvtitle" />

    <TextView
        android:id="@+id/prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="49dp"
        android:fontFamily="@font/nunito_italic"
        android:paddingLeft="15dp"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:text="hello"
        android:textColor="#0C0909"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvtitle"
        app:layout_constraintTop_toTopOf="parent" />

    <RatingBar
        android:id="@+id/rating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/prof"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-13dp"
        android:layout_marginEnd="1dp"
        android:isIndicator="true"
        android:numStars="5"
        android:scaleX="0.5"
        android:scaleY="0.5" />

    <TextView
        android:id="@+id/rate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/prof"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="33dp"
        android:text="4.5"
        android:textColor="#0C0000"
        android:textColorHint="#0C0000"
        android:textSize="14sp" />

    <ImageView
        android:id="@+id/call"
        android:layout_width="67dp"
        android:layout_height="47dp"
        android:layout_below="@+id/rating"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-6dp"
        android:layout_marginEnd="45dp"
        app:srcCompat="@drawable/call60" />

    <ImageView
        android:id="@+id/payment"
        android:layout_width="69dp"
        android:layout_height="53dp"
        android:layout_below="@+id/rating"
        android:layout_marginTop="-12dp"
        android:layout_marginEnd="22dp"
        android:layout_toStartOf="@+id/call"
        app:srcCompat="@drawable/wallet" />

    <ImageView
        android:id="@+id/map"
        android:layout_width="51dp"
        android:layout_height="53dp"
        android:layout_below="@+id/rating"
        android:layout_marginTop="-13dp"
        android:layout_marginEnd="28dp"
        android:layout_toStartOf="@+id/payment"
        app:srcCompat="@drawable/map" />

    <ImageView
        android:id="@+id/chat"
        android:layout_width="103dp"
        android:layout_height="58dp"
        android:layout_below="@+id/tvtitle"
        android:layout_alignParentStart="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="16dp"
        android:layout_toStartOf="@+id/map"
        app:srcCompat="@drawable/chat" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/chat"
        android:layout_marginTop="-7dp" />


</RelativeLayout>